<div class="container new-container">
  <h1 align="center">Welcome to <%= @user.username %>'s Page</h1>
  <div class="row">
    <div class="col-md-12 col-md-offset-4" align="center">
      <%= gravatar_for @user, size: 120 %>
    </div>
  </div>
  
  <h3 align="center"><%= @user.username %>'s Articles</h3>
  
 
  
  <%= render 'articles/article', obj: @user_articles %>
 
  
   <div align="center" class="center">
    <%= will_paginate @user_articles %>
  </div>
</div>

